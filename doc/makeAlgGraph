find ./ -name "*Alg.hpp" -print | xargs grep "public "  | sed -f makeAlgGraph.sed  |  sed -f makeAlgGraph2.sed   |  sed -f makeAlgGraph2.sed |  sed -f makeAlgGraph3.sed |  sed -f makeAlgGraph3.sed  | sed -f makeAlgGraph3.sed | sed 's/::/_/g' | sed 's/BasicBlockPreaccumulation/BBP/g' | sed 's/ControlFlowReversal/CFR/g' | grep -v AdjointAssembly > algGraphEdges.txt
find ../system/ -name "*AlgBase.hpp" -print | xargs grep "public " | sed 's/ virtual//g' | cut -d " " -f 4,6 | grep -v ^AlgBase | sed 's/AlgBase//g' | sed 's/^/Base_/g' | sed 's/: /->Base_/g' | sed -f makeAlgGraph.sed >> algGraphEdges.txt
cat algGraphEdges.txt | sed 's/->/\n/' | sed 's/[ 	]*//g' | sort | uniq > algGraphVertices.txt 
